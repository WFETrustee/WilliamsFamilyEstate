{
  "css": {
  "autoOrganize": true,
  "allowInlineOverrides": true,
  "excludeFromRootDistribute": [
    ".main-nav",
    ".site-header",
    ".site-footer",
    "#site-header",
    "#site-footer"
  ],
  "alwaysDistribute": [
    ".announce",
    ".content",
    ".date",
    ".meta-group",
    ".meta.title",
    ".meta.date"
  ]
  },
  "display": {
    "defaultDateFormat": "MMMM d, yyyy",
    "defaultLocale": "en-US",
    "enablePushpinIcon": true
  },
  "fonts": {
    "loadGoogleFonts": true,
    "googleFonts": [
      "Spectral+SC",
      "Playfair+Display",
      "Scope+One"
    ]
  },
  "performance": {
    "manifestCaching": true
  },
  "mode": {
    "debug": false,
    "publish": "live"
  },
  "automation": {
    "generateSitemap": true,
    "archiveToInternetArchive": false
  }
}
