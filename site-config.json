{
  "css": {
    "autoOrganize": true,
    "allowInlineOverrides": true
  },
  "display": {
    "defaultDateFormat": "MMMM d, yyyy",
    "defaultLocale": "en-US",
    "enablePushpinIcon": true
  },
  "fonts": {
    "loadGoogleFonts": true,
    "googleFonts": [
      "Spectral+SC",
      "Playfair+Display",
      "Scope+One"
    ]
  },
  "performance": {
    "manifestCaching": true
  },
  "mode": {
    "debug": false,
    "publish": "live"
  }
}